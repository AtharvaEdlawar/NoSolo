{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",    
        "dest": "index.js"
      },
      {
        "src": "/cosplayers",    
        "dest": "/index.html"
      },
      {
        "src": "/artist",
        "dest": "/index.html"
      },
      {
        "src": "/about",
        "dest": "/index.html"
      },
      {
        "src": "/tickets",
        "dest": "/index.html"
      },
      {
        "src": "/(.*)",       
        "dest": "/index.html"
      }
    ]
  }