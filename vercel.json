{
    "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/Backend/index.js"
    }
  ],

  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  
    "routes":[
        {
            "src":"/",
            "dest": "/"
        },
        {
           "src":"/cosplayers",
            "dest": "/"
        },
        {
            "src":"/artist",
             "dest": "/"
         },
         {
            "src":"/about",
             "dest": "/"
         },
         {
            "src":"/tickets",
             "dest": "/"
         }
    ]
}