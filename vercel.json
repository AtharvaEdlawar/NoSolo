{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",    
        "dest": "index.js"
      },
      {
        "src": "^/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/cosplayers",    
        "dest": "/index.html"
      },
      {
        "src": "/artist",
        "dest": "/index.html"
      },
      {
        "src": "/about",
        "dest": "/index.html"
      },
      {
        "src": "/tickets",
        "dest": "/index.html"
      },
      {
        "src": "/(.*)",       
        "dest": "/index.html"
      }
    ]
  }